<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>update_storedprocedures</servlet-name>
        <servlet-class>storedprocedures.update_storedprocedures</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RawQuery</servlet-name>
        <servlet-class>reports.RawQuery</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loadQueryHistory</servlet-name>
        <servlet-class>storedprocedures.loadQueryHistory</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>update_storedprocedures</servlet-name>
        <url-pattern>/update_storedprocedures</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RawQuery</servlet-name>
        <url-pattern>/RawQuery</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loadQueryHistory</servlet-name>
        <url-pattern>/loadQueryHistory</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
